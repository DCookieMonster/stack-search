{
  "name": "Stack Search",
  "description": "Enrich google search results. Adding: # of answer, and the rank of the top answer of StackOverFlow",
  "version": "1.1.1",
  "short_name": "Stack Search",
  "minimum_chrome_version": "16.0.884",
 "background": {
		"run_at": "document_end",
		"page": "html/background.html",
        "js":["js/background.js"],
        "persistent": false
    },    
  "content_scripts": [ {
    "run_at" : "document_end",
    "js": [ "js/jquery.js", "js/background.js"],
	"css" :[ ],
 "matches": [ "*://*/*" ],
        "include_globs": [
            "*://*.google.*/*"
                    ]
  }],
    "browser_action": {
      "default_title": "Stack Search",
      "default_icon": "icon.png",
      "default_popup": "html/popups.html"
  },
   "author": "dor amir",
  "permissions": ["tabs","<all_urls>"],
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
